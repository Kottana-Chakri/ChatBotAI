{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "src/lib/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "VITE_APP_NAME": "ConstructBot Professional",
    "VITE_GEMINI_API_KEY": "@gemini-api-key"
  }
}
